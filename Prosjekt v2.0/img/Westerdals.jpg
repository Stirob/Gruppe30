#nav1 ul li:before{
    content: '';
    width: 50px;
    height: 50px;
    top: 0;
    left: 0;
    background: white;
    border-radius: 50%;
    transform: scale(0);
    transition: .5s ease-in-out;
    position: absolute;
}

